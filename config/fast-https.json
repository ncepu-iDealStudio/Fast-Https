{
  "error_log": "logs/error.log notice",
  "pid": "logs/fast-https.pid",
  "http": {
    "include": "mime.types",
    "default_type": "application/octet-stream",
    "server": [
      {
        "listen": 8080,
        "server_name": "localhost",
        "limit": 20,
        "burst":20,
        "blaklist": ["192.168.1.1-192.168.1.99", "59.65.232.244,59.66.66.66", "88.88.8.8"],
        "location": [
          {
            "url": "/x{3}/999",
            "type": "local",
            "limit": {
              "mem":10,
              "rate":20,
              "burst":20
            },
            "root": "./httpdoc/root",
            "index":["index.html", "index.htm"] 
          }
        ]
      },
      {
        "listen": "443 ssl",
        "server_name": "localhost",
        "ssl_certificate": "config/cert/localhost.pem",
        "ssl_certificate_key": "config/cert/localhost-key.pem",
        "location": [
          {
            "url": "/",
            "type": "local",
            "root": "./httpdoc/root",
            "index":["index.html", "index.htm"] 
          }
        ]
      },
      {
        "listen": 8081,
        "server_name": "localhost",
        "location": [
          {
            "url": "/",
            "type": "proxy",
            "proxy_pass": "http://127.0.0.1:5000",
            "root": "./httpdoc/root",
            "index":["index.html", "index.htm"] 
          }
        ]
      },
      {
        "listen": 9090,
        "server_name": "localhost",
        "location": [
          {
            "url": "/",
            "type": "proxy",
            "proxy_pass": "http://club.quwancode.com",
            "proxy_cache": {
              "path": "/home/pzc/Project/Fast-https/fast-https/.cache",
              "valid": ["200:1h", "304:1h", "any:30m"],
              "key": "$request_method$host$request_uri",
              "max_size": 1024
            }
          }
        ]
      }
    ]
  }
}
