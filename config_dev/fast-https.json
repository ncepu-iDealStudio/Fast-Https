{
  "log_root":"./logs",
  "pid": "./fast-https.pid",
  "engine": {
    "is_master": false,
    "id": "A",
    "slave_ip": "127.0.0.1",
    "slave_port": 5001
  },
  "http": {
    "include": "mime.types",
    "servers_limit": {
      "limit": 1000000,
      "burst": 5000000
    },
    "log_split": " ",
    "log_format": [
      "_user_agent", "ip_port", "time", "type", "method", "path",
      "host", "status", "size", "referer"
    ],
    "blaklist": [
      "192.168.11.1-192.168.11.150", 
      "192.168.1.151,59.66.66.66", 
      "192.168.1.151",
      "192.168.0.135",
      "10.115.37.109"
    ],
    "server": [
      {
        "listen": 8080,
        "server_name": "",
        "location": [
          {
            "url": "/",
            "type": "local",
            "root": "./httpdoc/root",
            "index":["index.html", "index.htm"],
            "appfirewall": [
              "sql",
              "xss"
            ]
          }
        ]
      },
      {
        "listen": "4443 ssl h2",
        "server_name": "127.0.0.1",
        "ssl_certificate": "config/cert/127.0.0.1.pem",
        "ssl_certificate_key": "config/cert/127.0.0.1-key.pem",
        "location": [
          {
            "url": "/",
            "type": "devmod"
          }
        ]
      },
      {
        "listen": 9091,
        "server_name": "localhost",
        "location": [
          {
            "url": "/",
            "type": "proxy",
            "proxy_pass": "http://127.0.0.1:80",
            "proxy_set_header" : [
              {
                "HeaderKey": "Host",
                "HeaderValue": "$proxy_host"
              }
            ]
          }
        ]
      },
      {
        "listen": 9099,
        "server_name": "localhost",
        "location": [
          {
            "url": "/",
            "type": "proxy",
            "proxy_pass": "http://4.227.189.175:10086",
            "proxy_set_header" : [
              {
                "HeaderKey": "Host",
                "HeaderValue": "$proxy_host"
              }
            ],
            "appfirewall": [
              "sql",
              "xss"
            ]
          }
        ]
      },
      {
        "listen": 6060,
        "server_name": "localhost",
        "location": [
          {
            "url": "/",
            "type": "proxy",
            "proxy_pass": "http://127.0.0.1:7070"
            
          }
        ]
      },
      {
        "listen": 7000,
        "server_name": "localhost",
        "location": [
          {
            "url": "/api1",
            "type": "rewrite",
            "rewrite": "http://club.quwancode.com"
          }
        ]
      }
    ]
  }
}
